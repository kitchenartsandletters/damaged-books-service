                                        {% endif %}
                                    
                                        const handleSubmit = async () => {
                                          const email = emailInput.value.trim();
                                          const customerName = nameInput?.value.trim() || null;
                                          if (!email || !email.includes('@')) {
                                            statusEl.textContent = 'Please enter a valid email.';
                                            statusEl.style.color = 'red';
                                            return;
