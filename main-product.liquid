                                            statusEl.style.color = 'red';
                                          }
                                    
                                          submitBtn.disabled = false;
                                          submitBtn.textContent = originalText;
                                        };
                                    
                                        submitBtn?.addEventListener('click', handleSubmit);
                                    
                                        emailInput?.addEventListener('keydown', function (e) {
                                          if (e.key === 'Enter') {
                                            e.preventDefault();
                                            handleSubmit();
                                          }
                                        });
                                      });
                                    </script>
